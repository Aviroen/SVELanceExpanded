{
    "Format": "2.3.0",
    // C O N F I G
    "ConfigSchema": {
        "Override Generic Dialogue": {
            "AllowValues": "true, false",
            "Default": "true",
            "Section": "Content",
            "Description": "Overrides generic universal dialogue. Still alters certain dialogue like Welwick's predictions and summit dialogue. Please disable this if you aren't planning to date Lance. "
        },
        "Sprite": {
            "AllowValues": "Use included, Use custom",
            "Default": "Use included",
            "Section": "Appearance",
            "Description": "This toggles the Portrait + Sprite combination, if you toggle this off, you will be missing out on 90+ emotes and the dialogue will act strange. You have been warned."
        },
        "Use Beach Sprites": {
            "AllowValues": "true, false",
            "Default": "true",
            "Section": "Appearance",
            "Description": "Toggles on and off whether or not the game calls a beach sprite. Please change this to false if you do not have beach sprites and are using a custom sprite set."
        },
        "Earring Colour": {
            "AllowValues": "turquoise, red",
            "Default": "turquoise",
            "Section": "Appearance",
            "Description": "Changes the colour of Lance' eventual earring gift in the event. Please pick which suit you better. To actually wear them in game, please install the Fashion sense pack."
        },
        "Darker Plot Events(Y4)": {
            "AllowValues": "true, false",
            "Default": "true",
            "Section": "Content",
            "Description": "Enables the somewhat darker string of critical plot events where Lance' trauma response is on full display. If you would prefer a more light-hearted experience I suggest you turn this off. TW: Overt self-destructive  tendencies and vague suicidal tendency implications. Potentially may possess triggering scenarios. (The farmer is briefly kidnapped and sealed into silence about the event.) Toggling this off in the middle of said events will stop them from continuing so you may test it out to see if it's for you."
        },
        "Spicier Content": {
            "AllowValues": "Bell Pepper, Habanero, Carolina Reaper, Pepper X",
            "Default": "Habanero",
            "Section": "Content",
            "Description": "You are warned that Pepper X is [Aviroen] letting the muzzle off my writer who is a thirsty dog. Carolina Reaper is more explicit than Habanero, Habanero is akin to RRRR's 'wine event' spice. Bell Pepper is rated-E for everyone."
        },

        "HeartEvents": {
            "AllowValues": "LanceExpanded, pause",
            "Default": "pause",
            "Section": "Content",
            "Description": "Resets ALL of my mod's heart events, default 'inactive', toggle and save to activate."
        },

        "LanceNewRoom": {
            "AllowValues": "true, false",
            "Default": "true",
            "Section": "Content",
            "Description": "Changed Lance's room to be less morbid and actually useful for him lorewise."
        },
        "Custom Wedding": {
            "AllowValues": "true, false",
            "Default": "true",
            "Section": "Content",
            "Description": "Changes and mutes the original wedding event, adds Camilla in, moves it to Fable Reef. Specific to Lance. Turn this off if you're not marrying Lance."
        },
        "Aviroen Magnus": {
            "AllowValues": "true, false",
            "Default": "false",
            "Section": "Content",
            "Description": "Integrating just the basic clothes from my mod that followed Eskbl's art style for Magnus. Default Off."
        },
        "Day 1 Toggle": {
            "AllowValues": "true, false",
            "Default": "false",
            "Section": "Content",
            "Description": "Default, you will meet Lance after the volcano event, change to true if you want to meet Lance after the Wizard junimo note. If you turn on the day 1 toggle you will still need to get to the volcano for his heart events, but you can at least gift and marry him early."
        }
    },
